! first disable Keycap Picture insert mode
URxvt.iso14755:         false
URxvt.iso14755_52:      false
sURxvt*title: urxvt
URxvt*termName: rxvt-unicode-256color

! urxvt term by term ################################
URxvt*perl-lib:             /usr/lib/urxvt/perl
URxvt*perl-ext-common:      default,matcher,clipboard,keyboard-select,font-size
! ,tabbedex
URxvt*perl-matcher: 		1
URxvt*matcher-button:		3
URxvt.urlLauncher:          xdg-open
URxvt.urlButton:            1
URxvt.underlineURLs:            True
URxvt*matcher.button:           1
URxvt.saveLines:                5000
Urxvt*letterspace:              -5
URxvt*cursorBlink:		true
URxvt*scrollBar:		false
! URxvt*depth:			32 
URxvt*internalBorder: 		5
URxvt*dynamicColors: on

URxvt.scrollstyle: rxvt
urxvt*transparent: true
urxvt*shading: 0

! xterm 
!----------------------------------------------------------------------
xterm*faceName: xft:DejaVu Sans Mono for Powerline:pixelsize=14:antialias=true

Xft.antialias: true
Xft.autohint: false
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
! For Notify-send
URxvt*urgentOnBell: true
URxvt*visualBell:   false
! Font change settings
URxvt.font:           xft:Source Code Pro for Powerline:size=13:hinting:false, \ xft:PowerlineSymbols:size=9:antialias=true
! URxvt.imFont:         xft:Sauce Code Pro for Powerline:size=13, \ xft:PowerlineSymbols:size=9:antialias=true
! URxvt.boldFont:       xft:Sauce Code Pro for Powerline:size=13, \ xft:PowerlineSymbols:size=9:antialias=true
! URxvt.italicFont:     xft:Sauce Code Pro for Powerline:size=13, \ xft:PowerlineSymbols:size=9:antialias=true
! URxvt.boldItalicFont: xft:Sauce Code Pro for Powerline:size=13, \ xft:PowerlineSymbols:size=9:antialias=true


URxvt.intensityStyles: true
URxvt.boldMode:       false
URxvt.skipBuiltinGlyphs: false
URxvt.loginShell: true

URxvt.keysym.C-equal:	perl:font-size:increase
URxvt.keysym.C-minus:   perl:font-size:decrease
URxvt.keysym.C-S-equal:   perl:font-size:incglobal
URxvt.keysym.C-S-minus: perl:font-size:decglobal
URxvt.keysym.C-0:      perl:font-size:reset
Urxvt.font-size.step:  8


! Para cut and paste 
! URxvt.perl-ext-common: ...,clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-u: perl:url-select:select_next         
URxvt.keysym.M-Escape: perl:keyboard-select:activate  
! nose para q del copy and paste
URxvt.copyCommand:  xsel -ib
URxvt.pasteCommand: xsel -ob


!----TAB MANAGEMENT----------------------------------------------------------
! Urxvt.tabbed.new-button:        true

! URxvt.tabbed.no-tabbedex-keys:  true

! URxvt.keysym.Control-t: perl:tabbedex:new_tab
! URxvt.keysym.Control-Tab: perl:tabbedex:next_tab
! URxvt.keysym.Control-Shift-Tab: perl:tabbedex:prev_tab
! URxvt.keysym.Control-Shift-Left: perl:tabbedex:move_tab_left
! URxvt.keysym.Control-Shift-Right: perl:tabbedex:move_tab_right
! URxvt.keysym.Control-n: perl:tabbedex:rename_tab


